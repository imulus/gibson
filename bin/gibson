#!/usr/bin/env bash

libs=( brew/install.sh
       brew/formulae.sh
       git/install.sh
       git/config.sh
       npm/packages.sh
       zsh/install.sh
       ruby/install.sh
       ruby/gems.sh
       ssh/keygen.sh )

for lib in ${libs[@]}
do
  curl -sL https://raw.github.com/imulus/gibson/master/lib/$lib | bash
done
